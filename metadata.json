{
  "name": "sensson-r1soft",
  "version": "1.1.0",
  "author": "sensson",
  "license": "Apache-2.0",
  "summary": "A R1Soft Server Backup Manager Puppet module",
  "source": "https://github.com/sensson/puppet-r1soft",
  "tags": [ "idera", "r1soft", "backup" ],
  "operatingsystem_support": [
    { 
